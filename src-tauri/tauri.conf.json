{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Stellar VPN",
  "version": "0.1.0",
  "identifier": "org.stellarsecurity.vpn.desktop",
  "build": {
    "beforeBuildCommand": "npm run build:web",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
