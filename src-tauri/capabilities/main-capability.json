{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "main-capability",
  "description": "Main window capability",
  "windows": ["main"],
  "permissions": [
    "core:path:default",
    "core:event:default",
    "core:window:default",
    "core:app:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",

    "store:default",
    "os:default",
    "process:default",
    "updater:default",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-is-enabled",

    "fs:default"
  ]
}
